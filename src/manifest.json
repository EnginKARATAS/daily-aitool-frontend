{
  "name": "vue in Content Scripts",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_icon": {
          "16": "images/icon-16.png",
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
      },
      "default_title": "Vue Popup"
  },
  "icon-s": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content-scripts/main.js"]
      }
  ]
}